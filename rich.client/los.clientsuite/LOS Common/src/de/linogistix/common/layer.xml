<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="de-linogistix-common-action-OpenMobileAction.instance"/>
            <file name="de-linogistix-common-bobrowser-browse-BOBrowserAction.instance"/>
            <file name="de-linogistix-common-action-OpenHelpAction.instance"/>
            <file name="de-linogistix-common-action-OnlineHelpAction.instance"/>
        </folder>
        <folder name="BOBrowser">
            <!-- Add refresh action for all business objects -->
            <folder name="MasterActions">
                <file name="de-linogistix-common-bobrowser-action-RefreshBOBeanNodeAction.instance"/>
            </folder>
            <folder name="User">
                <folder name="MasterActions">
                    <file name="de-linogistix-common-bobrowser-action-BOLockAction.instance"/>
                </folder>
            </folder>
            <folder name="JasperReports">
                <folder name="MasterActions">
                    <file name="de-linogistix-common-bobrowser-action-BOJasperReportLoadSourceAction.instance">
                        <attr name="position" intvalue="1"/>
                    </file>
                    <file name="de-linogistix-common-bobrowser-action-BOJasperReportSaveSourceAction.instance">
                        <attr name="position" intvalue="2"/>
                    </file>
                    <file name="de-linogistix-common-bobrowser-action-BOJasperReportCompileAction.instance">
                        <attr name="position" intvalue="5"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="JavaHelp">
            <file name="common-helpset.xml" url="common-helpset.xml">
                <attr name="position" intvalue="1000"/>
            </file>
        </folder>
    </folder>

    <!-- Configuration of generic LOS Entity Browser -->
    <folder name="BOBrowser">
        <folder name="System">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="de.linogistix.common.res.Bundle"/>
            <attr name="position" intvalue="100"/>
            <file name="de-linogistix-common-bobrowser-bo-BOClient.instance" >
                <attr name="position" intvalue="100"/>
            </file>
            <file name="de-linogistix-common-bobrowser-bo-BOUser.instance" >
                <attr name="position" intvalue="200"/>
            </file>
            <file name="de-linogistix-common-bobrowser-bo-BORole.instance" >
                <attr name="position" intvalue="300"/>
            </file>
            <file name="de-linogistix-common-bobrowser-bo-BOLOSSystemProperty.instance" >
                <attr name="showInExplorer" boolvalue="true"/>
                <attr name="position" intvalue="500"/>
            </file>
            <file name="de-linogistix-common-bobrowser-bo-BOJasperReport.instance" >
                <attr name="position" intvalue="700"/>
            </file>
        </folder>
    </folder>

    <folder name="Menu">
        
        <!-- Hide original Netbeans menus -->
        <file name="File_hidden"/>
        <file name="Edit_hidden"/>
        <file name="GoTo_hidden"/>
        <file name="Source_hidden"/>
        <file name="Window_hidden"/>
        <file name="View_hidden"/>
        <file name="Tools_hidden"/>
        <file name="Help_hidden"/>
        
        <!-- File menu -->
        <folder name="LOSFile">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="de.linogistix.common.res.Bundle"/>
                <attr name="position" intvalue="100"/>
                <file name="org-netbeans-core-actions-SystemExit.shadow">
                        <attr name="originalFile" stringvalue="Actions/System/org-netbeans-core-actions-SystemExit.instance"/>
                        <attr name="position" intvalue="1000"/>
                </file>
        </folder>
        
        <!-- Window menu -->
        <folder name="LOSWindow">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="de.linogistix.common.res.Bundle"/>
            <attr name="position" intvalue="200"/>

            <file name="BOBrowserAction.shadow">
                    <attr name="position" intvalue="10"/>
                    <attr name="originalFile" stringvalue="Actions/Window/de-linogistix-common-bobrowser-browse-BOBrowserAction.instance"/>
            </file>
            <file name="Sep-099[javax-swing-JSeparator].instance">
                <attr name="position" intvalue="099"/>
            </file>

            <file name="Sep-899[javax-swing-JSeparator].instance">
                <attr name="position" intvalue="899"/>
            </file>
            <file name="OpenMobileAction.shadow">
                <attr name="position" intvalue="950"/>
                <attr name="originalFile" stringvalue="Actions/Window/de-linogistix-common-action-OpenMobileAction.instance"/>
            </file>
        </folder>
        
        <!-- Administration menu -->
        <folder name="LOSActions">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="de.linogistix.common.res.Bundle"/>
            <attr name="position" intvalue="300"/>
        </folder>

        <!-- Tools menu -->
        <folder name="LOSTools">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="de.linogistix.common.res.Bundle"/>
            <attr name="position" intvalue="450"/>
            <file name="org-netbeans-modules-options-OptionsWindowAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-options-OptionsWindowAction.instance"/>
                <attr name="position" intvalue="1700"/>
            </file>
            <file name="org-netbeans-core-io-ui-IOWindowAction.shadow">
                    <attr name="position" intvalue="2000"/>
                    <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-core-io-ui-IOWindowAction.instance"/>
            </file>

        </folder>

        <!-- Help menu -->
        <folder name="LOSHelp">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="de.linogistix.common.res.Bundle"/>
            <attr name="position" intvalue="500"/>
            <!--
            <file name="OpenHelpAction.shadow">
                <attr name="position" intvalue="100"/>
                <attr name="originalFile" stringvalue="Actions/Window/de-linogistix-common-action-OpenHelpAction.instance"/>
            </file>
            -->
            <file name="OnlineHelpAction.shadow">
                <attr name="position" intvalue="200"/>
                <attr name="originalFile" stringvalue="Actions/Window/de-linogistix-common-action-OnlineHelpAction.instance"/>
            </file>
            <file name="Sep-299[javax-swing-JSeparator].instance">
                <attr name="position" intvalue="299"/>
            </file>
			
        </folder>

    </folder>
    
    <!-- Hide original Netbeans toolbars -->
    <folder name="Toolbars">
        <file name="Edit_hidden"/>
        <file name="File_hidden"/>
        <file name="Memory_hidden"/>
        <file name="Clipboard_hidden"/>
        <file name="UndoRedo_hidden"/>
        <file name="Standard.xml_hidden"/>
    </folder>
    
    <!-- Add connection properties to the netbeans option dialog -->
    <folder name="OptionsDialog">
        <file name="Advanced.instance_hidden"/>
        <file name="AppServerOptionsCategory.instance">
            <attr name="instanceClass" stringvalue="de.linogistix.common.preferences.AppServerOptionsCategory"/>
        </file>
        <file name="JNDIOptionsCategory.instance">
            <attr name="instanceClass" stringvalue="de.linogistix.common.preferences.JNDIOptionsCategory"/>
        </file>
        <file name="de-linogistix-common-preferences-DELETEOptionsCategory.instance"/>
    </folder>
	
    <!-- Definition of the window layout -->
    <folder name="Windows2">
        <folder name="Components">
            <file name="BOBrowserTopComponent.settings" url="./bobrowser/browse/BOBrowserTopComponentSettings.xml"/>

            <!-- Hide runtime tab 1 -->
            <file name="runtime.settings_hidden"/>
        </folder>
        <folder name="Modes">
            <folder name="editor" >
            </folder>
            <folder name="explorer">
                <file name="BOBrowserTopComponent.wstcref" url="./bobrowser/browse/BOBrowserTopComponentWstcref.xml"/>

                <!-- Hide runtime tab 2 -->
                <!--<file name="runtime.wstcref_hidden"/>-->
            </folder>
            <folder name="properties" />
            <folder name="rightSlidingSide" />
        </folder>
    </folder>

</filesystem>
